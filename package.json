{
  "name": "girocode",
  "version": "1.0.0",
  "description": "A simple and tiny library for generating GiroCode (EPC QR code) strings",
  "author": {
    "name": "Thomas Rasshofer",
    "email": "hello@thomasrasshofer.com",
    "url": "https://thomasrasshofer.com/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rasshofer/girocode.git"
  },
  "bugs": {
    "url": "https://github.com/rasshofer/girocode/issues"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "girocode",
    "epc",
    "qr"
  ],
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf 'dist' 'coverage'",
    "build": "tsc --build --verbose",
    "watch": "tsc --build --verbose --watch",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/iban": "^0.0.32",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "iban": "^0.0.14"
  }
}
